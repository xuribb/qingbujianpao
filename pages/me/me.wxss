page{
  background-color: #eeeeee;
}

.header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #16b777;
  padding: 20px 0;
}
.header_left{
  padding-left: 25px;
  display: flex;
}
.header_left>image{
  width: 50px;
  height: 50px;
  margin-right: 10px;
  border-radius: 5px;
}
.header_left>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.level{
  font-size: 12px;
  margin-top: 5px;
}
.header_right{
  padding-right: 25px;
  width: 25px;
  height: 25px;
}

.main{
  height: 210px;
  background-color: #FFFFFF;
  margin: 20px;
  border-radius: 3px;
  box-shadow: 5px 5px 10px 0 rgba(0,0,0,0.1);
}
.main_item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 20px;
  padding-top: 20px;
  padding-bottom: 10px;
  border-bottom: 1px solid #16b777;
}
.main_item>view{
  display: flex;
  align-items: center;
}
.main_item_logo{
  width: 25px;
  height: 25px;
  margin-right: 5px;
}
.main_item_tou{
  width: 15px;
  height: 15px;
}

.footer{
  margin-top: 300rpx;
  text-align: center;
  font-size: 12px;
  color: #aaaaaa;
}